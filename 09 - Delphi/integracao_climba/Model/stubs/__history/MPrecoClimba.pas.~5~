unit MPrecoClimba;

interface

type
  TMPrecoClimba = class
    private
      Fname: string;
      Fid: string;
      procedure Setid(const Value: string);
      procedure Setname(const Value: string);
    public
      property id: string read Fid write Setid;
      property name: string read Fname write Setname;
      constructor Create();
      destructor Destroy(); override;
  end;

implementation

{ TMPrecoClimba }

constructor TMPrecoClimba.Create;
begin

end;

destructor TMPrecoClimba.Destroy;
begin

  inherited;
end;

procedure TMPrecoClimba.Setid(const Value: string);
begin
  Fid := Value;
end;

procedure TMPrecoClimba.Setname(const Value: string);
begin
  Fname := Value;
end;

end.
